{

  "description": "",
  "manifest_version": 2,
  "name": "Buybye",
  "version": "1.0",
  "homepage_url": "https://byebuy.de",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["buybye.js"]
    }
  ],

  "options_ui": {
    "page": "options.html"
  },

  "permissions": ["storage"],

  "browser_specific_settings": {
    "gecko": {
      "id": "de.byebuy"
    }
  }

}
